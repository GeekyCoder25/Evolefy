{
	"expo": {
		"name": "Evolefy",
		"slug": "Evolefy",
		"version": "1.0.0",
		"orientation": "portrait",
		"icon": "./assets/images/icon.png",
		"scheme": "evolefy",
		"userInterfaceStyle": "automatic",
		"newArchEnabled": true,
		"ios": {
			"supportsTablet": true
		},
		"android": {
			"adaptiveIcon": {
				"foregroundImage": "./assets/images/adaptive-icon.png",
				"backgroundColor": "#ffffff"
			},
			"edgeToEdgeEnabled": true,
			"package": "com.evolefy.app",
			"googleServicesFile": "./google-services.json"
		},
		"androidNavigationBar": {
			"visible": "leanback",
			"barStyle": "light-content",
			"backgroundColor": "#0B0F1C"
		},
		"web": {
			"bundler": "metro",
			"output": "static",
			"favicon": "./assets/images/favicon.png"
		},
		"plugins": [
			"expo-notifications",
			"expo-router",
			[
				"expo-splash-screen",
				{
					"image": "./assets/images/splash-icon.png",
					"imageWidth": 200,
					"resizeMode": "contain",
					"backgroundColor": "#0B0F1C"
				}
			]
		],
		"experiments": {
			"typedRoutes": true
		},
		"extra": {
			"router": {},
			"eas": {
				"projectId": "86dd0156-a5c5-43ed-9691-d5d0c2eec6d2"
			}
		},
		"runtimeVersion": {
			"policy": "appVersion"
		},
		"updates": {
			"url": "https://u.expo.dev/86dd0156-a5c5-43ed-9691-d5d0c2eec6d2"
		}
	}
}
